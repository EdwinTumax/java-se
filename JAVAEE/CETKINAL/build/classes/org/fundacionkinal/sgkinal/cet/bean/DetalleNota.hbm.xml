<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.cet.bean.DetalleNota" table="detalle_nota">
    <id column="idDetalle" name="idDetalle">
      <generator class="identity"/>
    </id>
    <many-to-one column="idActividad" name="detalleActividades"/>
    <property column="carne" name="carne"/>
    <property column="valor" name="valor"/>
  </class>
</hibernate-mapping>
