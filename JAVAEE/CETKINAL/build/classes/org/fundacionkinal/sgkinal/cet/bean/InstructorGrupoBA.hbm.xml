<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.cet.bean.InstructorGrupoBA" table="instructor_grupo_ba">
    <id column="idInstructorGrupoBA" name="idInstructorGrupoBA">
      <generator class="identity"/>
    </id>
    <many-to-one class="org.fundacionkinal.sgkinal.cet.bean.InstructorAsignatura" column="idAsig" name="instructorAsignatura"/>
    <many-to-one class="org.fundacionkinal.sgkinal.cet.bean.GrupoBACET"  column="idGrupoBA" name="grupoBA" lazy="false"/>
    <many-to-one class="org.fundacionkinal.sgkinal.cet.bean.Salon" column="idSalon" name="salon" lazy="false"/>
  </class>
</hibernate-mapping>
