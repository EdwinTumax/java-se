<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.cet.bean.Clave" table="clave">
    <id column="idClave" name="idClave" type="java.lang.Integer">
      <generator class="identity"/>
    </id>
    <many-to-one column="clave" name="controlNota"/>
    <property column="numeroPregunta" name="numeroPregunta" type="java.lang.Integer"/>
    <property column="valor" name="valor" type="java.lang.Long"/>
    <property column="respuesta" name="respuesta" type="java.lang.String"/>
  </class>
</hibernate-mapping>
