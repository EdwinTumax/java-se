<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.aspirante.bean.PreguntaCuestionario" table="pregunta_cuestionario">
    <id column="idPreguntaCuestionario" name="idPreguntaCuestionario">
      <generator class="identity"/>
    </id>
    <property column="preguntaPrincipal" name="preguntPrincipal" type="java.lang.String"/>
    <property column="observaciones" name="observaciones" type="java.lang.String"/>
  </class>
</hibernate-mapping>
