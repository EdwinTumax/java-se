<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.aspirante.bean.Escuela" table="escuela">
    <id column="idEscuela" name="idEscuela">
      <generator class="identity"/>
    </id>
    <property column="escuela" name="escuela"/>
  </class>
</hibernate-mapping>
