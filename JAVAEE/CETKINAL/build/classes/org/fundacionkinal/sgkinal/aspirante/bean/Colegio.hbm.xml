<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.aspirante.bean.Colegio" table="colegio">
    <id column="idColegio" name="idColegio">
      <generator class="identity"/>
    </id>
    <property column="nombre" name="nombre" type="java.lang.String"/>
    <property column="observaciones" name="observaciones" type="java.lang.String"/>
  </class>
</hibernate-mapping>