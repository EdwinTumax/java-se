<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.moodle.bean.MdlGroupMember" table="mdl_groups_members">
    <id column="id" name="idMember">
      <generator class="identity"/>
    </id>
    <property column="groupid" name="idGroup" type="integer"/>
    <property column="userid" name="idUser" type="integer"/>
  </class>
</hibernate-mapping>
