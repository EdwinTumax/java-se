<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.moodle.bean.MdlGroup" table="mdl_groups">
    <id column="id" name="idGroup">
      <generator class="identity"/>
    </id>
    <property column="name" name="nombre" type="string"/>
    <property column="courseid" name="courseId" type="integer"/>
  </class>
</hibernate-mapping>
