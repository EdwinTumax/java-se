<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.moodle.bean.MdlQuizAttempts" table="mdl_quiz_attempts">
    <id column="id" name="idAttempts">
      <generator class="identity"/>
    </id>
    <property column="quiz" name="idQuiz" type="integer"/>
    <property column="userid" name="idUser" type="integer"/>
    <property column="sumgrades" name="nota" type="integer"/>
  </class>
</hibernate-mapping>
