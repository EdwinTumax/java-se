<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.contabilidad.bean.Cliente" table="cliente">
    <id column="nit" name="nit">
      <generator class="assigned"/>
    </id>
    <property column="nombre" name="nombre"/>
    <property column="direccion" name="direccion"/>
    <property column="telefono" name="telefono"/>
    <property column="email" name="email"/>
  </class>
</hibernate-mapping>
