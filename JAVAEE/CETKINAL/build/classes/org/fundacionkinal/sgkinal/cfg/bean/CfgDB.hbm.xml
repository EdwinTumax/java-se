<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.fundacionkinal.sgkinal.cfg.bean.CfgDB" table="cfgDB">
    <id column="idConfiguracion" name="idConfiguracion">
      <generator class="identity"/>
    </id>
    <property column="ipServidor" name="ipServidor"/>
    <property column="puerto" name="puerto"/>
    <property column="carpetaFoto" name="carpetaFoto"/>
  </class>
</hibernate-mapping>
